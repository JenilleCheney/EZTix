    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>5109576c-d240-4ce8-8d33-ee9e66a3364a</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="9.0.0" />
